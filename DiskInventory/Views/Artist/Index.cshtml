@model List<Artist>
@{
    ViewBag.Title = "Artist Manager";

}
<!--
    Original Author: Luke Chulack
    Date Created:  11/12/2021
    Version: 1.0
    Date Last Modified: 11/12/2021
    Modified by: Luke Chulack
    Modification log:

           version 1.0 -  11/12/2021  -  Artist view which builds the table that displays the Artist data.

    -->
<h1>@ViewBag.title</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Artist ID</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var artist in Model)
        {
            <tr>
                <td>@artist.ArtistId</td>
                <td>@artist.ArtistLname</td>
                <td>@artist.ArtistFname</td>
                <td>@artist.ArtistTypeId</td>
            </tr>
        }
    </tbody>
</table>